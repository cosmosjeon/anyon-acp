{
  "name": "anyon",
  "private": true,
  "version": "0.0.8",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,yellow\"",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm start",
    "dev:clean": "npm run clean:cache && npm run dev",
    "dev:bun": "concurrently \"bun run dev:frontend\" \"bun run dev:backend\" \"bun run tauri:dev\" --names \"frontend,backend,tauri\" --prefix-colors \"cyan,yellow,magenta\"",
    "tauri:dev": "tauri dev --config src-tauri/tauri.bun.conf.json --no-dev-server-wait",
    "dev:bun:clean": "bun run clean:cache && bun run dev:bun",
    "clean:cache": "rm -rf node_modules/.vite dist .parcel-cache",
    "kill:servers": "bash scripts/kill-dev-servers.sh",
    "kill:servers:win": "powershell -ExecutionPolicy Bypass -File scripts/kill-dev-servers.ps1",
    "tauri:bun": "tauri dev --config src-tauri/tauri.bun.conf.json",
    "build": "tsc && vite build",
    "build:bun": "bun run build",
    "prebuild": "",
    "build:executables": "bun run scripts/fetch-and-build.js --version=1.0.41",
    "build:executables:current": "bun run scripts/fetch-and-build.js current --version=1.0.41",
    "build:executables:linux": "bun run scripts/fetch-and-build.js linux --version=1.0.41",
    "build:executables:macos": "bun run scripts/fetch-and-build.js macos --version=1.0.41",
    "build:executables:windows": "bun run scripts/fetch-and-build.js windows --version=1.0.41",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:dmg": "tauri build --bundles dmg",
    "check": "tsc --noEmit && cd src-tauri && cargo check",
    "prepare": "husky",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-virtual": "^3.13.10",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-global-shortcut": "^2.3.1",
    "@tauri-apps/plugin-http": "^2.5.4",
    "@tauri-apps/plugin-opener": "^2.5.2",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "@types/diff": "^8.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@uiw/react-md-editor": "^4.0.7",
    "ansi-to-html": "^0.7.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "diff": "^8.0.2",
    "framer-motion": "^12.0.0-alpha.1",
    "html2canvas": "^1.4.1",
    "hugeicons-react": "^0.3.0",
    "posthog-js": "^1.258.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^7.10.1",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.14.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.8",
    "zod": "^3.24.1",
    "zustand": "^5.0.6",
    "zwitch": "^2.0.4"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.5",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/sharp": "^0.32.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "sharp": "^0.34.2",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide"
  ],
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.25.6",
    "@rollup/rollup-linux-x64-gnu": "^4.45.1"
  },
  "overrides": {
    "yallist": "3.1.1"
  }
}
